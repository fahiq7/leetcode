class Solution(object):
    def twoSum(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: List[int]
        """
        hash_map = {}  
        for i, val in enumerate(nums):
            complement = target - val 
            if complement in hash_map:
                return [hash_map[complement], i]
            hash_map[val] = i
